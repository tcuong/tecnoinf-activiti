      <documentation>Por favor ingrese la cedula del estudiante para realizar la inscripcion.</documentation>
      <extensionElements>
        <activiti:formProperty id="IngresoCedula" name="Ingreso la cÃ©dula del estudiante" type="string" variable="cedula" required="true" readable="true" writable="true"></activiti:formProperty>
      </extensionElements>
    </userTask>
