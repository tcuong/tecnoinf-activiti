    </sequenceFlow>
    <sequenceFlow id="flow9" name="PaseValido" sourceRef="exclusivegateway2" targetRef="seleccionarCarrera">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${paseValido==false}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow10" name="" sourceRef="seleccionarCarrera" targetRef="servicetask3"></sequenceFlow>
    <sequenceFlow id="flow12" name="" sourceRef="usertask4" targetRef="endevent1"></sequenceFlow>
    <userTask id="mostrarMensaje" name="Mostrar mensaje">
      <documentation>${msj}</documentation>
